{
  "name": "react-native-hybrid",
  "version": "0.0.2",
  "description": "A React Boilerplate to share code between Web, iOS and Android applications",
  "main": "gulpfile.js",
  "scripts": {
    "start": "npm run env-web && ./node_modules/gulp/bin/gulp.js default",
    "serve": "forever start ./node_modules/http-server/bin/http-server -p 80 -d false",
    "native-start": "adb reverse tcp:8081 tcp:8081 & npm run env-native && npm run native-watch & npm run native-packager",
    "native-watch": "watch 'npm run native-copy' src/js",
    "native-copy": "npm run native-clean && cp -R src/js ApplicationNative/ApplicationNative/app",
    "native-clean": "rm -rf ApplicationNative/ApplicationNative/app",
    "native-packager": "./ApplicationNative/ApplicationNative/node_modules/react-native/packager/packager.sh",
    "env-web": "NODE_ENV=web node ./node_tasks/env_task.js",
    "env-native": "NODE_ENV=native node ./node_tasks/env_task.js"
  },
  "keywords": [
    "react",
    "react native",
    "iOS",
    "android",
    "hybrid",
    "share code",
    "cross platform"
  ],
  "authors": [
    "Simon Holmes (https://github.com/srsholmes)",
    "Joe Harlow (https://github.com/f5io/)",
    "Brian Daviadoff (https://github.com/FThreeNiiX)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.7.5",
    "babel-eslint": "^5.0.0",
    "babel-plugin-react-transform": "2.0.0-beta1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-preset-react-native": "^1.5.6",
    "babel-runtime": "^5.5.6",
    "browser-sync": "^2.7.13",
    "browserify": "^13.0.0",
    "express": "^4.13.1",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-if": "^1.2.5",
    "gulp-notify": "^2.2.0",
    "gulp-sass": "^2.0.x",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^1.2.0",
    "gulp-watch": "^4.3.5",
    "http-server": "^0.8.0",
    "livereactload": "latest",
    "morgan": "^1.6.1",
    "react-addons-test-utils": "^0.14.6",
    "react-proxy": "^1.1.2",
    "run-sequence": "^1.1.1",
    "uglifyify": "^3.0.1",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watch": "^0.17.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babelify": "7.2.0",
    "mixin": "^0.2.0",
    "react": "^0.14.5",
    "react-dom": "^0.14.5",
    "react-native": "^0.23.1",
    "react-redux": "^4.0.0",
    "react-router": "2.0.0",
    "react-router-redux": "^4.0.0",
    "redux": "^3.0.4",
    "redux-logger": "^2.0.4",
    "to-fast-properties": "^1.0.2",
    "whatwg-fetch": "^0.9.0"
  }
}
